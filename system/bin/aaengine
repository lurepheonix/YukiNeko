#!/system/bin/sh
#  AC!D AUDIO ENGINE TWEEKS BY knight47
#  Plz don't edit this if you don't know what you are doing
#  This file will check wheather important files are loaded

clear
echo "";
echo "  ------------->  AA Engine 8 for ICS+<----------";
echo "  ------------->       Team AC!D      <----------";
echo "  -->Fuser1337,Knight47,Krabappel2548,R-ikfoot,RizalLovinsSundanesse,ThilinaC<--";
echo "";
echo "";
sleep 5
echo "";
echo "";
echo " Modifying build.prop...... ";
echo " Please be patient...... ";
sleep 1
sleep 1
sleep 1
echo "";
echo "";
echo "";
echo "";
echo " Modifying AC!D Sound Tweaks by TEAM AC!D(Version 8) ";
busybox echo "###AC!D Sound Tweaks by TEAM AC!D(Version 8)###" >> /system/build.prop
busybox echo "setprop htc.audio.global.speaker=1" >> /system/build.prop
busybox echo "setprop ro.qc.sdk.audio.ssr=false" >> /system/build.prop
busybox echo "setprop ro.qc.sdk.audio.fluencetype=none" >> /system/build.prop
busybox echo "setprop ro.service.swiqi2.supported=true" >> /system/build.prop
busybox echo "setprop persist.service.swiqi2.enable=1" >> /system/build.prop
busybox echo "setprop persist.audio.vr.enable=false" >> /system/build.prop
busybox echo "setprop persist.htc.audio.pcm.samplerate=48000" >> /system/build.prop
busybox echo "setprop persist.audio.SupportHTCHWAEC=1" >> /system/build.prop
echo " Modifying Sony Xloud & Clearbass + ";
busybox echo "#Sony Xloud & Clearbass +" >> /system/build.prop
busybox echo "setprop ro.semc.sound_effects_enabled=true" >> /system/build.prop
busybox echo "setprop ro.semc.xloud.supported=true" >> /system/build.prop
busybox echo "setprop ro.semc.enhance.supported=true" >> /system/build.prop
busybox echo "setprop ro.semc.clearaudio.supported=true" >> /system/build.prop
busybox echo "setprop ro.sony.walkman.logger=1" >> /system/build.prop
busybox echo "setprop ro.somc.clearphase.supported=true" >> /system/build.prop
echo " Modifying Resampling ";
busybox echo "#Resampling" >> /system/build.prop
busybox echo "setprop ro.audio.samplerate=48000" >> /system/build.prop
busybox echo "setprop ro.audio.pcm.samplerate=48000" >> /system/build.prop
busybox echo "setprop persist.audio.samplerate=48000" >> /system/build.prop
busybox echo "setprop persist.af.resample=48000" >> /system/build.prop
busybox echo "setprop persist.af.resampler.quality=255" >> /system/build.prop
busybox echo "setprop persist.dev.pm.dyn_samplingrate=1" >> /system/build.prop
echo " Modifying MPQAudioPlayer ";
busybox echo "#System prop to select MPQAudioPlayer by default on mpq8064" >> /system/build.prop
busybox echo "setprop mpq.audio.decode=true" >> /system/build.prop
echo "";
echo "";
echo "";
echo "";
sleep 4
echo "TEAM AC!D @xda-devs!";
sleep 4
echo " Modifying Awesome Beats Engine ";
busybox echo "#Awesome Beats Engine" >> /system/build.prop
busybox echo "setprop htc.audio.global.state=0" >> /system/build.prop
busybox echo "setprop htc.audio.lpa.a2dp=0" >> /system/build.prop
busybox echo "setprop htc.audio.global.profile=0" >> /system/build.prop
busybox echo "setprop htc.audio.q6.topology=0" >> /system/build.prop
busybox echo "setprop htc.audio.enable_dmic=1" >> /system/build.prop
busybox echo "setprop htc.audio.swalt.mingain=14512" >> /system/build.prop
busybox echo "setprop htc.audio.swalt.enable=1" >> /system/build.prop
busybox echo "setprop htc.audio.alc.enable=1" >> /system/build.prop
busybox echo "setprop persist.audio.fluence.mode=endfire" >> /system/build.prop
busybox echo "setprop persist.audio.hp=true" >> /system/build.prop
busybox echo "setprop persist.htc.audio.pcm.samplerate=44100" >> /system/build.prop
busybox echo "setprop persist.htc.audio.pcm.channels=2" >> /system/build.prop
echo "";
echo "";
echo "";
echo "";
sleep 4
echo " Modified.Please reboot to take effect ";
echo " THANKS FOR CHOOSING AC!D AUDIO Engine ";
echo " end ";
sleep 2
echo "";
echo "";
echo "";